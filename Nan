function sandycheeks (num, args){
    return num * args
}

const trysome = (arg,args) =>{
    return new Promise((resolve,reject)=>{

        if(!isNaN(arg)  || !isNaN(args)){
            reject("Not a number")
        }

        const result = sandycheeks(arg,args)
        let error = false

        if(result>=100) {
            error = true
        }

        if(!error){
            resolve(result)
        }

        reject("value was higher than the max threshold")
    })
}

function message(a){
console.log(a)
}

trysome("g",10).then((e)=>message(e)).catch(e=>{console.log(e)})